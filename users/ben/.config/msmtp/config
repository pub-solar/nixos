account hello@benjaminbaedorf.eu
  host mail.hosting.de
  port 587
  protocol smtp
  auth on
  from hello@benjaminbaedorf.eu
  user hello@benjaminbaedorf.eu
  tls on
  tls_trust_file /etc/ssl/certs/ca-certificates.crt

